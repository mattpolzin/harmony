. ../setup.sh

export IDRIS2_PACKAGE_PATH=$IDRIS2_PACKAGE_PATH:$(realpath ../depends):$(realpath ../../depends)

idris2 -p harmony \
  -p contrib \
  -p json \
  -p algebra \
  -p array \
  -p ref1 \
  -p linear \
  -p refined \
  -p bytestring \
  -p ilex-core \
  -p parser-json \
  -p parser \
  -p elab-util \
  -p idris-adds \
  --exec main Graph.idr

rm -rf build
